#include<bits/stdc++.h>
using namespace std;
int main()
{
    int a[5],b=4,s1,s2,s3,s4,c=0,s5,s6;
    for(int i=0;i<b;i++)
    {
        cin>>a[i];
    }
    sort(a,a+b);
    s1=a[0]+a[3];
    s2=a[1]+a[2];
    s5=a[3]-a[0];
    s3=a[0]+a[2];
    s4=a[3]-a[1];

     if(s1==s2||s2==s5||s3==s4) cout<<"YES";
    else cout<<"NO";
}
